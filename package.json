{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev --hostname myhost --port 3333",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "yarn lint:eslint && yarn lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "lintfix": "eslint . --fix && prettier --write --list-different .",
    "typecheck-changed": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/eslint": "^0.4.0",
    "@nuxt/image": "^1.8.0",
    "@pinia/nuxt": "^0.5.3",
    "nuxt": "^3.12.4",
    "nuxt-icons": "^3.2.1",
    "nuxt-swiper": "^1.2.2",
    "pinia": "^2.2.1",
    "vue": "latest"
  },
  "devDependencies": {
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "typescript": "^5.5.4",
    "vue-tsc": "^2.0.29"
  }
}
